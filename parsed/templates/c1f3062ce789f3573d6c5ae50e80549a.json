{
  "components/products/product-view": "<div class=\"productView themevale_productView\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" data-entity-id=\"{{product.id}}\" data-name=\"{{product.title}}\" data-product-category=\"{{#each product.category}}{{#if @last}}{{this}}{{else}}{{this}}, {{/if}}{{/each}}\" data-product-brand=\"{{product.brand.name}}\" data-product-price=\"{{#if product.price.with_tax}}{{product.price.with_tax.value}}{{else}}{{product.price.without_tax.value}}{{/if}}\" data-product-variant=\"single-product-option\"{{/if}}>\n    {{#each product.reviews.messages}}\n        {{#if error}}\n            {{> components/common/alert-error error}}\n        {{/if}}\n        {{#if success}}\n            {{> components/common/alert-success success}}\n        {{/if}}\n    {{/each}}\n    \n    <div class=\"themevale_productView-left\">\n        <div class=\"themevale_productView-images zoom-image\" data-image-gallery>\n            {{#if product.main_image}}\n                <meta itemprop=\"name\" content=\"{{product.main_image.data}}\" />\n            {{/if}}\n            <div class=\"themevale_badges\">\n                {{#each products.new}}\n                    {{#if this.id '==' ../product.id}}\n                        <div class=\"new-badge themevale_badge\">\n                            <span class=\"text\">New</span></div>\n                    {{/if}}\n                {{/each}}\n                {{#or product.price.non_sale_price_with_tax product.price.non_sale_price_without_tax}}\n                    <div class=\"sale-badge themevale_badge\">\n                        <span class=\"text\">Sale</span>\n                    </div>\n                {{/or}}\n                {{#each product.custom_fields}}\n                    {{#if name '===' 'custom label'}}\n                    <div class=\"custom-badge themevale_badge\">\n                        <span class=\"text\">{{{value}}}</span>\n                    </div>\n                    {{/if}}\n                {{/each}}\n                {{#if product.out_of_stock_message}}\n                    <div class=\"soldOut-badge themevale_badge\">\n                        <span class=\"text\">sold out</span>\n                    </div>\n                {{/if}}\n            </div>\n            <div class=\"productView-nav-wrapper\">\n               {{#if product.images.length}}\n                    <div class=\"productView-nav\" data-slick='{\n                        \"fade\": true,\n                        \"arrows\": false,\n                        \"dots\": false,\n                        \"infinite\": true,\n                        \"slidesToShow\": 1,\n                        \"slidesToScroll\": 1,\n                        \"asNavFor\": \".themevale_productView-images .productView-for\"\n                    }'>\n                        {{#each product.images}}\n                            <figure class=\"productView-image\" data-fancybox=\"images\" data-href=\"{{getImage this 'zoom_size' (cdn ../theme_settings.default_image_product)}}\" \n                                data-image-gallery-main\n                                data-zoom-image=\"{{getImage this 'zoom_size' (cdn theme_settings.default_image_product)}}\"\n                                data-mfp-src=\"{{getImage this 'zoom_size' (cdn theme_settings.default_image_product)}}\">\n                                <div class=\"productView-img-container\">\n                                    <a href=\"{{getImage this 'zoom_size' (cdn theme_settings.default_image_product)}}\">\n                                        <img class=\"productView-image--default\"\n                                            data-sizes=\"auto\" src=\"{{getImage this 'zoom_size' (cdn ../theme_settings.default_image_product)}}\"\n                                            alt=\"{{this.alt}}\" title=\"{{this.alt}}\" data-main-image>\n                                    </a>\n                                </div>\n                            </figure>\n                        {{/each}}\n                    </div>\n               {{else}}\n                    <div class=\"productView-nav noImage\">\n                        <figure class=\"productView-image is-error\">\n                            <div class=\"productView-img-container\">\n                                <img class=\"productView-image--default lazyload\"\n                                 data-sizes=\"auto\"\n                                 src=\"{{cdn 'img/loading.svg'}}\"\n                                 data-src=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\"\n                                 alt=\"{{product.main_image.alt}}\" title=\"{{product.main_image.alt}}\" data-main-image>\n                            </div>\n                        </figure>\n                    </div>\n                {{/if}}\n                {{#if product.videos.list.length}}\n                    <div class=\"label-videos\">\n                        <a class=\"video-link\" href=\"javascript:void(0);\" data-reveal-id=\"video-gallery-popup\" aria-label=\"Show Product Video Popup\">\n                            <img class=\"icon\" src=\"https://cdn11.bigcommerce.com/s-3tntjfynbx/product_images/uploaded_images/btn-play.png\" alt=\"Video Icon\"></span>\n                        </a>\n                        {{>components/themevale/modals/video-gallery product.videos}}\n                    </div>\n                {{/if}}\n                {{#if theme_settings.enable_compare_color}}\n                    <div class=\"compare-color\">\n                        <a class=\"compareColor-link\" href=\"javascript:void(0);\" data-reveal-id=\"compare-color-popup\">\n                            {{lang 'common.compare_color'}}\n                        </a>\n                        {{> components/products/compare-color}}\n                    </div>\n                {{/if}}\n            </div>\n            {{#if product.images.length}}\n                <div class=\"productView-for\" data-slick='{\n                    \"arrows\": true,\n                    \"dots\": false,\n                    \"infinite\": true,\n                    \"focusOnSelect\": true,\n                    \"slidesToShow\": 5,\n                    \"slidesToScroll\": 1,\n                    \"asNavFor\": \".themevale_productView-images .productView-nav\",\n                    \"nextArrow\": \"<svg class=&apos;slick-next slick-arrow&apos; aria-label=&apos;Next Product Image&apos;><use xlink:href=&apos;#icon-slick-next&apos;></use></svg>\", \n                    \"prevArrow\": \"<svg class=&apos;slick-prev slick-arrow&apos; aria-label=&apos;Previous Product Image&apos;><use xlink:href=&apos;#icon-slick-prev&apos;></use></svg>\",\n                    \"responsive\": [\n                        {\n                            \"breakpoint\": 1025,\n                            \"settings\": {\n                                \"slidesToShow\": 4,\n                                \"slidesToScroll\": 1\n                            }\n                        },\n                        {\n                            \"breakpoint\": 415,\n                            \"settings\": {\n                                \"slidesToShow\": 4,\n                                \"slidesToScroll\": 1\n                            }\n                        },\n                        {\n                            \"breakpoint\": 321,\n                            \"settings\": {\n                                \"slidesToShow\": 3,\n                                \"slidesToScroll\": 1\n                            }\n                        }\n                    ]\n                }'>\n                    {{#each product.images}}\n                        <div class=\"productView-thumbnail\">\n                            <a\n                                class=\"productView-thumbnail-link\"\n                                href=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\"\n                                data-image-gallery-item\n                                data-image-gallery-new-image-url=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\"\n                                data-image-gallery-zoom-image-url=\"{{getImage this 'zoom_size' (cdn ../theme_settings.default_image_product)}}\">\n                                <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage this 'productthumb_size' (cdn ../theme_settings.default_image_product)}}\" alt=\"{{this.alt}}\" title=\"{{this.alt}}\">\n                            </a>\n                        </div>\n                    {{/each}}\n                </div>\n            {{/if}}    \n        </div>\n    </div>\n    <div class=\"themevale_productView-right\">\n        <div class=\"productView-details\">\n            <div class=\"productView-product\">\n                {{#if product.brand}}\n                    <h2 class=\"productView-brand\"{{#if schema}} itemprop=\"brand\" itemscope itemtype=\"http://schema.org/Brand\"{{/if}}>\n                        <a href=\"{{product.brand.url}}\"{{#if schema}} itemprop=\"url\"{{/if}}><span{{#if schema}} itemprop=\"name\"{{/if}}>{{product.brand.name}}</span></a>\n                    </h2>\n                {{/if}}\n                <h1 class=\"productView-title\" {{#if schema}}itemprop=\"name\"{{/if}}>{{product.title}}</h1>\n                <div class=\"productView-code-wrap\">\n                    <div class=\"productView-code\">\n                        {{#if product.sku}}\n                            <dt class=\"productView-info-name\">Product Code:</dt>\n                            <dd class=\"productView-info-value VariationProductSKU\" data-product-sku itemprop=\"sku\">{{product.sku}}</dd>\n                        {{/if}}\n                    </div>\n                    <div class=\"productView-code\">\n                        {{#if product.upc}}\n                            <dt class=\"productView-info-name\">UPC Code:</dt>\n                            <dd class=\"productView-info-value\" data-product-upc>{{product.upc}}</dd>\n                        {{/if}}\n                    </div>\n                </div>\n                <p class=\"productView-shortDescription\">Kinedyne’s 2” x 30’ Chain Anchor Ratchet Strap is engineered to be able to withstand harsh conditions while\n                    keeping your load secure and contained. With a working load limit of 3,335 Lbs. / 1,513 Kgs., the Chain Anchor\n                    Ratchet Strap is equipped with a heat-treated carbon steel standard handle ratchet for ease and comfort.\n                </p>\n                <div class=\"prodcutView-form\">\n                    <p class=\"prodcutView-form--title\">Have Questions?</p>\n                    <h4 class=\"prodcutView-form--heading\">Fill out the form below</h4>\n                    <form class=\"form\" id=\"contact_form\">\n                        <fieldset class=\"form-fieldset\">\n                            <div class=\"form-field\">\n                                <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_name\" placeholder=\"Full Name\" value=\"{{customer.name}}\">\n                            </div>\n                            <div class=\"form-field\">\n                                <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_email\" placeholder=\"Email\" value=\"{{customer.email}}\">\n                            </div>\n                            <div class=\"form-field\">\n                                <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_phone\" placeholder=\"Phone\" value=\"{{customer.phone}}\">\n                            </div>                        \n                            <div class=\"form-field\">\n                                <input required=\"true\" type=\"text\" class=\"form-input\" name=\"customer_location\" placeholder=\"Location\" value=\"{{customer.phone}}\">\n                            </div>                        \n                            <div class=\"form-field\">\n                                <textarea name=\"message\" class=\"form-input\" id=\"comment-area\" placeholder=\"Message\" required=\"true\"></textarea>\n                            </div>                        \n                            <div class=\"form-actions\">\n                                <button type=\"button\" class=\"btn btn-primary\" id=\"ask-an-expert-button\">\n                                    <span class=\"cargoRight-item-btn--text btn-text\">Submit</span>\n                                    <span class=\"cargoRight-item-btn--icon btn-icon\">\n                                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-custom-arrow\"></use></svg>\n                                    </span>\n                                </button>\n                            </div>\n                        </fieldset>\n                    </form>\n                </div>\n                \n\n            </div>\n        </div>\n    </div>\n    <article class=\"productView-description\">\n        {{#if theme_settings.show_product_details_tabs}}\n            {{> components/products/description-tabs}}\n        {{else}}\n            {{> components/products/description  settings=../settings}}\n        {{/if}}\n    </article>\n</div>\n\n<div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n\n<script type=\"text/javascript\">\n    $(document).ready(function() {\n        $(\"#ask-an-expert-button\").click(function() {\n            var ask_proceed = true;\n            //simple validation at client's end\n            //loop through each field and we simply change border color to red for invalid fields\n            $(\"#contact_form input[required=true], #contact_form textarea[required=true]\").each(function() {\n                if (!$.trim($(this).val())) { //if this field is empty\n                    $(this).parent('.form-field').removeClass('form-field--success').addClass('form-field--error');\n                    ask_proceed = false; //set do not proceed flag\n                } else {\n                    $(this).parent('.form-field').removeClass('form-field--error').addClass('form-field--success');\n                }\n                //check invalid email\n                var email_reg = /^([\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4})?$/;\n                if ($(this).attr(\"name\") == \"customer_email\" && !email_reg.test($.trim($(this).val()))) {\n                    $(this).parent('.form-field').removeClass('form-field--success').addClass('form-field--error');\n                    ask_proceed = false; //set do not proceed flag\n                }\n            });\n\n            if (ask_proceed) //everything looks good! proceed...\n            {\n                //get input field values data to be sent to server\n                ask_post_data = {\n                    'api': 'i_send_mail',\n                    'subject':   '{{theme_settings.themevale_ask_an_expert_subject}}',\n                    'from_name': '{{theme_settings.themevale_ask_an_expert_name}}',\n                    'email':     '{{theme_settings.themevale_ask_an_expert_email}}',\n                    'email_from': $('#contact_form input[name=customer_email]').val(),\n                    'message': '<h2>ASK AN EXPERT</h2><table>\\\n                            <tr><td><strong>Your Name</strong></td><td>' + $('#contact_form input[name=customer_name]').val() + '</td></tr>\\\n                            <tr><td><strong>Email Address</strong></td><td>' + $('#contact_form input[name=customer_email]').val() + '</td></tr>\\\n                            <tr><td><strong>Phone Number</strong></td><td>' + $('#contact_form input[name=customer_phone]').val() + '</td></tr>\\\n                            <tr><td><strong>Location</strong></td><td>' + $('#contact_form input[name=customer_location]').val() + '</td></tr>\\\n                            <tr><td><strong>What can i help you with today ?</strong></td><td>' + $('#contact_form textarea[name=message]').val() + '</td></tr>\\\n                        </table>'\n                };\n\n                //Ajax post data to server\n                $.post('https://themevale.net/tools/sendmail/ask-an-expert/', ask_post_data, function(response) {\n                    if (response.type == 'error') { //load json data from server and output message\n                        output = '<div class=\"error\">' + response.text + '</div>';\n                    } else {\n                        output = '<div class=\"alert alert-success\">Thank you. We\\'ve received your feedback and will respond shortly.</div>';\n                        //reset values in all input fields\n                        $(\"#contact_form  input[required=true], #contact_form textarea[required=true]\").val('');\n                        $(\".form-inner\").hide(); //hide form after success\n                    }\n                    $(\"#contact_results\").hide().html(output).show();\n                }, 'json');\n            }\n        });\n\n        //reset previously set border colors and hide all message on .keyup()\n        $(\"#contact_form input[required=true], #contact_form textarea[required=true]\").keyup(function() {\n            if (!$.trim($(this).val())) { //if this field is empty\n                $(this).parent('.form-field').removeClass('form-field--success').addClass('form-field--error');\n                ask_proceed = false; //set do not proceed flag\n            } else {\n                $(this).parent('.form-field').removeClass('form-field--error').addClass('form-field--success');\n            }\n            //check invalid email\n            var email_reg = /^([\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4})?$/;\n            if ($(this).attr(\"name\") == \"customer_email\" && !email_reg.test($.trim($(this).val()))) {\n                $(this).parent('.form-field').removeClass('form-field--success').addClass('form-field--error');\n                ask_proceed = false; //set do not proceed flag\n            }\n            $(\"#contact_results\").hide();\n        });\n    });\n</script>",
  "components/common/alert-error": "<div class=\"alertBox alertBox--error\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n",
  "components/common/alert-success": "<div class=\"alertBox alertBox--success\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n",
  "components/themevale/modals/video-gallery": "<div id=\"video-gallery-popup\" class=\"modal\" data-reveal>\r\n    <div class=\"modal-content\">\r\n        <a href=\"#\" class=\"modal-close\" aria-label=\"Close\"><span aria-hidden=\"true\">&#215;</span></a>\r\n        <div class=\"modal-header\">\r\n            <h2 class=\"modal-header-title\">video gallery</h2>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n            <div class=\"videoGallery themevale_videoGallery\" data-video-gallery>\r\n                <div id=\"videoGallery-content\">\r\n                    <div class=\"videoGallery-main\">\r\n                        <iframe\r\n                            id=\"player\"\r\n                            class=\"lazyload\"\r\n                            type=\"text/html\"\r\n                            width=\"640\"\r\n                            height=\"390\"\r\n                            frameborder=\"0\"\r\n                            webkitAllowFullScreen\r\n                            mozallowfullscreen\r\n                            allowFullScreen\r\n                            data-src=\"//www.youtube.com/embed/{{this.featured.id}}\"\r\n                            data-video-player>\r\n                        </iframe>\r\n                    </div>\r\n                    <ul class=\"videoGallery-list\">\r\n                    {{#each this.list}}\r\n                        <li class=\"videoGallery-item\">\r\n                            <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\r\n                                <div class=\"video-figure\">\r\n                                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\r\n                                </div>\r\n                            </a>\r\n                        </li>\r\n                    {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n            <div class=\"loadingOverlay\"></div>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/products/compare-color": "<div id=\"compare-color-popup\" class=\"modal\" data-reveal>\r\n    <div class=\"modal-content\">\r\n        <a href=\"#\" class=\"modal-close\" aria-label=\"Close\">&times;</a>\r\n        <div class=\"modal-header\">\r\n            <h2 class=\"modal-header-title\">{{lang 'common.compare_color_text'}}</h2>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n            <div class=\"themevaleCompareColor\" data-compare-color>\r\n                <div class=\"compareColor-swatch\"></div>\r\n                <div id=\"color-swatch-image\" class=\"color-swatch-image scrollbar-horizontal\"></div>\r\n            </div>\r\n            <div class=\"loadingOverlay\"></div>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/products/description-tabs": "<ul class=\"tabs\" data-tab role=\"tablist\">\n    <li class=\"tab is-active\">\n        <a class=\"tab-title\" href=\"#tab-description\">Detail</a>\n    </li>\n    <li class=\"tab\">\n        <a class=\"tab-title\" href=\"#tab-specifications\">Specifications</a>\n    </li>\n    <li class=\"tab\">\n        <a class=\"tab-title\" href=\"#tab-packaging\">Packaging</a>\n    </li>\n    <li class=\"tab\">\n        <a class=\"tab-title\" href=\"#tab-notices\">Notices & Warnings</a>\n    </li>\n</ul>\n<div class=\"tabs-contents\">\n    <div class=\"tab-content productDescription is-active\" id=\"tab-description\" data-accordion-item=\"\">\n        <div class=\"toggle-title\">\n            <a class=\"toggleLink\" href=\"#tab-description-content\" data-collapsible>\n                <span class=\"title\">Detail</span>\n                <span class=\"icon\"><svg aria-label=\"Show/Hide Description Tab\"><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n            </a>\n        </div>\n        <div class=\"toggle-content is-open\" id=\"tab-description-content\" aria-hidden=\"false\"{{#if schema}} itemprop=\"description\"{{/if}}>\n            <div class=\"productDescription-content\">\n                <div class=\"productDescription-left\">\n                    <h4 class=\"productDescription-title\">2-Inch x 30-Foot Chain Anchor Ratchet Strap</h4>\n                    <p class=\"productDescription-content\">With grab hooks fixed on the ends of the chain, your tie down jobs are even more secure on both sides of your load with\n                        the Kinedyne 2-inch x 30-foot Chain Anchor Ratchet Strap with a working load limit of 3,335 Lbs. / 1,513 Kgs.\n                    </p>\n                    <p class=\"productDescription-content\">Our easily identifiable Kinedyne Gold with Black Edge 2084F Webbing lets you know that this Chain Anchor Ratchet\n                        Strap is both strong and reliable with a standard 18” fixed end. Golden dichromate zinc plated heat-treated carbon steel\n                        Kinedyne 3705-2 Chain Anchors with 5/16 inch Grade 43 chain hooks are all the hardware our 11.1 pound / 5.0 kilogram 2”\n                        x 30’ Chain Anchor Ratchet Strap needs in order to withstand some of nature’s strongest challenges to ensure that your\n                        load remains secure.\n                    </p>\n                    <p class=\"productDescription-content\">Kinedyne tightly weaves its own webbing and designs the sew patterns for our 2” X 30’ Chain Anchor Ratchet Strap\n                        webbing. We assume direct quality control over the manufacturing in order to certify that the ratchet straps exceed the\n                        demands of many tie down applications.\n                    </p>\n                    <p class=\"productDescription-content\">The Kinedyne 533039 has a tested working load limit of 3,335 Lbs. / 1,513 Kgs. Our Ratchet Straps are stenciled and\n                        tagged with the Kinedyne name and logo as well as the Working Load Limit to meet DOT regulations and CVSA, CHP,\n                        North American Cargo Securement, and WSTDA standards.\n                    </p>\n                </div>\n    \n                <div class=\"productDescription-right\">\n                    <h4 class=\"productDescription-title\">2” by 30’ Chain Anchor Ratchet Strap – 533039 Quick Specs:</h4>\n                    <p class=\"productDescription-content\"><strong>Working Load Limit:</strong> 3,335 Lbs. / 1,513 Kgs</p>\n                    <p class=\"productDescription-content\"><strong>Width:</strong> 2 Inches</p>\n                    <p class=\"productDescription-content\"><strong>Length: </strong> 30 Feet</p>\n                    <p class=\"productDescription-content\"><strong>Weight: </strong> 11.1 Lbs. / 5.0 Kgs.</p>\n                    <p class=\"productDescription-content\"><strong>Webbing: </strong> Standard Kinedyne Gold with Black Edge 2084F Webbing.</p>\n                    <p class=\"productDescription-content\"><strong>Fixed End Length: </strong> 18 Inches.</p>\n                    <p class=\"productDescription-content\"><strong>Type of Tension Device: </strong> Kinedyne 802 Standard Handle Ratchet</p>\n                    <p class=\"productDescription-content\"><strong>End Fitting: </strong> Kinedyne 3705-2 Chain Anchor</p>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"tab-content productCustomTab\" id=\"tab-specifications\" data-accordion-item=\"\">\n        <div class=\"toggle-title\">\n            <a class=\"toggleLink\" href=\"#tab-specifications-content\" data-collapsible>\n                <span class=\"title\">Specifications</span>  \n                <span class=\"icon\"><svg aria-label=\"Show/Hide Specifications\"><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n            </a>\n        </div>\n        <div class=\"toggle-content\" id=\"tab-specifications-content\" aria-hidden=\"true\">\n            {{> components/themevale/tabs/tab-custom}}\n        </div>\n    </div>\n    <div class=\"tab-content productCustomTab\" id=\"tab-packaging\" data-accordion-item=\"\">\n        <div class=\"toggle-title\">\n            <a class=\"toggleLink\" href=\"#tab-packaging-content\" data-collapsible>\n                <span class=\"title\">Packaging</span>  \n                <span class=\"icon\"><svg aria-label=\"Show/Hide Packaging\"><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n            </a>\n        </div>\n        <div class=\"toggle-content\" id=\"tab-packaging-content\" aria-hidden=\"true\">\n            {{{region name = \"product_tab\"}}}\n        </div>\n    </div>\n\n    <div class=\"tab-content productCustomTab\" id=\"tab-notices\" data-accordion-item=\"\">\n        <div class=\"toggle-title\">\n            <a class=\"toggleLink\" href=\"#tab-notices-content\" data-collapsible>\n                <span class=\"title\">Notices & Warnings</span>  \n                <span class=\"icon\"><svg aria-label=\"Show/Hide Notices & Warnings\"><use xlink:href=\"#icon-chevron-down\"></use></svg></span>\n            </a>\n        </div>\n        <div class=\"toggle-content\" id=\"tab-notices-content\" aria-hidden=\"true\">\n            {{{region name = \"product_tab\"}}}\n        </div>\n    </div>\n</div>\n",
  "components/products/description": "<p class=\"productView-title\">{{lang 'products.description'}}</p>\n<div class=\"productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}{{#if schema}} itemprop=\"description\"{{/if}}>\n    {{{product.description}}}\n    {{{snippet 'product_description'}}}\n</div>\n\n{{#if product.warranty}}\n    <p class=\"productView-title\">{{lang 'products.warranty'}}</p>\n    <div class=\"productView-description\">\n        {{{product.warranty}}}\n    </div>\n{{/if}}\n",
