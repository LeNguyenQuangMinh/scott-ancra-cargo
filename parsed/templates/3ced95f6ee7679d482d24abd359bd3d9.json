{
  "components/themevale/header/header-layout-1": "<header class=\"header themevale_header-layout-1\">\r\n    <div class=\"themevale_header-PC\">\r\n        <div class=\"themevale_header-top\">\r\n            {{> components/common/navigation}}\r\n        </div>\r\n        <div class=\"themevale_header-middle\">\r\n            {{> components/themevale/header/header-middle}}\r\n        </div>\r\n        <div class=\"themevale_header-bottom menu-sticky\">\r\n            <div class=\"logo-wrapper\">\r\n                <div class=\"header-sticky-logo\">\r\n                    <a href=\"{{urls.home}}\">\r\n                        {{#if settings.store_logo.image}}\r\n                            <img class=\"header-logo-image lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/logo-whitecolor.png\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n                        {{else}}\r\n                            <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n                        {{/if}}\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            {{> components/common/navigation-menu}}\r\n            {{> components/common/sticky-cart-search}}\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale_header-Mobile\">\r\n        {{> components/themevale/header/header-mobile}}\r\n    </div>\r\n</header>\r\n",
  "components/common/navigation": "<nav class=\"navUser container text-right\" role=\"navigation\" aria-label=\"Header Navigation\">\n    <div class=\"navUser-item\">\n        <a class=\"navUser-action\" href=\"tel:{{settings.phone_number}}\">Customer Service {{settings.phone_number}}</a>\n    </div>\n    {{#if settings.gift_certificates_enabled}}\n    <div class=\"navUser-item\">\n        <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\n    </div>\n    {{/if}}\n    <div class=\"navUser-item navUser-item--account\">\n        {{#if customer}}\n            <a class=\"navUser-action navUser-action--account\" href=\"{{urls.account.index}}\">\n                {{lang 'common.myaccount'}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n            </a>\n            <ul class=\"dropdown-menu\" id=\"navPages-account-topbar\">\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\n                </li>\n                {{#if settings.returns_enabled}}\n                    <li class=\"dropdown-menu-item\">\n                        <a href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\n                    </li>\n                {{/if}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.inbox}}\">{{lang 'account.mobile_nav.messages'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\n                </li>\n                {{#if settings.show_payment_methods}}\n                    <li class=\"dropdown-menu-item\">\n                        <a href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\n                    </li>\n                {{/if}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.wishlists.all}}\">{{lang 'account.mobile_nav.wishlists'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\n                </li>\n                <li class=\"dropdown-menu-item\">\n                <a href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            </li>\n            </ul>\n        {{else}}\n            <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">{{lang 'common.myaccount'}}</a>\n        {{/if}}\n    </div>\n    <div class=\"navUser-item navUser-item--currencySelector\">\n        {{> components/common/currency-selector}}\n    </div>\n    <div class=\"navUser-item navUser-item--quickSearch\">\n        {{> components/common/quick-search}}\n    </div>\n</nav>\n",
  "components/themevale/header/header-middle": "<div class=\"container\">\r\n    <div class=\"headerMiddle-item text-left\">\r\n        <div class=\"logo-wrapper\">\r\n            {{#if page_type '===' 'default'}}\r\n                <h1 class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </h1>\r\n            {{else}}\r\n                <div class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n    <div class=\"headerMiddle-item text-right\">\r\n        <div class=\"own-text\">\r\n            <p>Free shipping on All Orders. No Minimum Purchase</p>\r\n        </div>\r\n        <div class=\"own-link\">\r\n            <div class=\"navUser-item navUser-item--cart themevale_cart\">\r\n                <a class=\"navUser-action\"\r\n                    data-cart-preview\r\n                    data-options=\"align:right\"\r\n                    href=\"{{urls.cart}}\" aria-label=\"{{lang 'common.cart'}}\">\r\n                    <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                    </div>\r\n                    <span class=\"navUser-text navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\r\n                    <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n                </a>\r\n                <div class=\"dropdown-cart\" id=\"cart-preview-dropdown\" aria-hidden=\"true\"></div>\r\n            </div>\r\n            <div class=\"navUser-item navUser-item--wishList\">\r\n                <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\" aria-label=\"{{lang 'common.wishlist'}}\">\r\n                    <div class=\"navUser-icon\">\r\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-heart\"></use></svg>\r\n                    </div>\r\n                    <span class=\"navUser-text\">{{lang 'common.wishlist'}}</span>\r\n                </a>\r\n            </div>\r\n            <div class=\"navUser-item navUser-item--account\">\r\n                {{#if customer}}\r\n                    <a class=\"navUser-action\" href=\"{{urls.account.index}}\"><strong>{{customer.name}}.</strong></a>\r\n                    <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\r\n                {{else}}\r\n                    <a class=\"navUser-action\" href=\"javascript:void(0)\" data-login-form>{{lang 'common.login'}}</a>\r\n                    {{#if settings.account_creation_enabled}}\r\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span>\r\n                    <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n                    {{/if}}\r\n                    {{#if page_type '!==' 'login'}}\r\n                        {{> components/common/login-form}}\r\n                    {{/if}}\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/common/navigation-menu": "<div class=\"navPages-container\" id=\"menu\" data-menu>\r\n    <nav class=\"navPages\">\r\n        <ul class=\"navPages-list navPages-list-depth-max{{#if theme_settings.themevale_megamenu}} navPages-list-megamenu{{/if}}\" data-level-list>\r\n            {{#each categories}}\r\n                <li class=\"navPages-item{{#if children}} has-dropdown{{else}} navPages-action-end{{/if}}\" data-level=\"2\">\r\n                    {{> components/common/navigation-list-alternate}}\r\n                </li>\r\n            {{/each}}\r\n            {{#unless theme_settings.hide_content_navigation}}\r\n                {{#each pages}}\r\n                    <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{else}} navPages-action-end{{/if}}\" data-level=\"2\">\r\n                        {{> components/common/navigation-list-alternate}}\r\n                    </li>\r\n                \r\n                 {{/each}}\r\n            {{/unless}}\r\n        </ul>\r\n\r\n        <ul class=\"navPages-list navPages-list--user\" data-level-list>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"{{urls.account.wishlists.all}}\">\r\n                    <span class=\"text\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-heart-2\"/></svg><span>{{lang 'account.mobile_nav.wishlists'}}</span></span>\r\n                </a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"/contact-us/\">\r\n                    <span class=\"text\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-location\"/></svg><span>Store Locator</span></span>\r\n                </a>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\">\r\n                    <span class=\"text\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-telephone\"/></svg><span>Contact us: {{settings.phone_number}}</span></span>\r\n                </a>\r\n            </li>\r\n            {{#if settings.gift_certificates_enabled}}\r\n                <li class=\"navPages-item\">\r\n                    <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\">\r\n                        <span class=\"text\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-gift\"/></svg><span>{{lang 'common.gift_cert'}}</span></span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n            {{#if currency_selector.currencies.length '>' 1}}\r\n                <li class=\"navPages-item currencies\" data-level=\"2\">\r\n                    <a id=\"currency_selector\" class=\"navPages-action has-subMenu\" href=\"javascript:void(0)\">\r\n                        <span class=\"text\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n                        <span class=\"navPages-action-moreIcon\">\r\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                        </span>\r\n                    </a>\r\n                    <div class=\"navPage-subMenu level-2\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                        <ul class=\"navPage-subMenu-list\">\r\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\" data-level=\"2\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"javascript:void(0)\">\r\n                                    <span class=\"navPages-action-moreIcon\">\r\n                                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                                        <span>Back</span>\r\n                                    </span>\r\n                                    <span class=\"text\">{{{currency_selector.active_currency_flag}}} {{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n                                </a>\r\n                            </li>\r\n                            {{#each currency_selector.currencies}}\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end\" data-level=\"3\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                    <span class=\"text\">\r\n                                    {{#if is_active}}\r\n                                        {{{flag}}} {{name}}\r\n                                    {{else}}\r\n                                        {{{flag}}} {{name}}\r\n                                    {{/if}}\r\n                                    </span>\r\n                                </a>\r\n                            </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                    </div>\r\n                </li>\r\n            {{/if}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        </ul>\r\n    </nav>\r\n</div>\r\n",
  "components/common/sticky-cart-search": "<div class=\"navUser-wrapper\">\r\n    <div class=\"navUser-item navUser-item--search\">\r\n        <a class=\"navUser-action\" href=\"#\" data-search=\"quickSearch\" aria-label=\"{{lang \"search.quick_search.button_search\"}}\" aria-expanded=\"false\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n    <div class=\"navUser-item navUser-item--cart themevale_cart\">\r\n        <a class=\"navUser-action\"\r\n            data-cart-preview\r\n            data-options=\"align:right\"\r\n            href=\"{{urls.cart}}\" aria-label=\"{{lang 'common.cart'}}\">\r\n            <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n            </div>\r\n            <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n        </a>\r\n        <div class=\"dropdown-cart\" id=\"cart-preview-dropdown-2\" aria-hidden=\"true\"></div>\r\n    </div>\r\n</div>\r\n",
  "components/themevale/header/header-mobile": "<div class=\"header-Mobile-item text-left\">\r\n    <div class=\"items item--hamburger\">\r\n        <a class=\"mobileMenu-toggle\" href=\"#\" data-mobile-menu-toggle=\"menu\" aria-label=\"Menu\">\r\n            <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n        </a>\r\n    </div>\r\n    <div class=\"items item--searchMobile\">\r\n        <a class=\"navUser-action\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\" aria-label=\"Show Search Form\"><use xlink:href=\"#icon-search\"></use></svg>\r\n                <svg class=\"icon icon-close\" aria-label=\"Close Search Form\"><use xlink:href=\"#times\"></use></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n</div>\r\n<div class=\"header-Mobile-item text-center\">\r\n    <div class=\"items\">\r\n        <div class=\"logo-wrapper\">\r\n            {{#if page_type '===' 'default'}}\r\n                <h1 class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </h1>\r\n            {{else}}\r\n                <div class=\"header-logo\">\r\n                    {{> components/common/store-logo}}\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</div>\r\n<div class=\"header-Mobile-item text-right\">\r\n    <div class=\"items item--account\">\r\n        <a class=\"navUser-action accountMobile\" href=\"javascript:void(0)\" aria-label=\"Show Account Popup\">\r\n            <div class=\"navUser-icon\">\r\n                <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-user\"/></svg>\r\n            </div>\r\n        </a>\r\n    </div>\r\n    <div class=\"items item--cart themevale_cart\">\r\n        <a class=\"navUser-action cartMobile\"\r\n            data-cart-preview\r\n            data-options=\"align:right\"\r\n            href=\"{{urls.cart}}\" aria-label=\"Show Cart Popup\">\r\n            <div class=\"navUser-icon navUser-item-cartIcon\">\r\n                <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\r\n                <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\r\n            </div>\r\n        </a>\r\n    </div>\r\n</div>\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<a class=\"navUser-action navUser-action--currencySelector\" href=\"javascript:void(0)\"\n    data-dropdown=\"currencySelection\" \n    aria-controls=\"currencySelection\" \n    aria-expanded=\"false\"\n>\n    <span class=\"image-flag\">{{{currency_selector.active_currency_flag}}}</span> \n    <span class=\"text-flag\">{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\n    <i class=\"icon\" aria-hidden=\"true\"><svg aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n</a>\n<ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n    {{#each currency_selector.currencies}}\n    <li class=\"dropdown-menu-item\">\n        <a href=\"{{{switch_url}}}\" data-currency-code=\"{{{code}}}\"\n            {{#if cart_currency_switch_url.length '>' 0 }}\n                data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n            {{/if}}\n            data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n        >\n            {{#if is_active}}\n            {{{flag}}} <strong>{{name}}</strong>\n            {{else}}\n            {{{flag}}} {{name}}\n            {{/if}}\n        </a>\n    </li>\n    {{/each}}\n</ul>\n{{/if}}\n",
  "components/common/quick-search": "{{{ snippet \"forms_search\"}}}\n<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_label'}}\" autocomplete=\"off\">\n             <button class=\"button-submit\" type=\"submit\" aria-label=\"{{lang \"search.quick_search.button_search\"}}\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n<div class=\"quickSearchResults\" data-bind=\"html: results\"></div>\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <span class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </span>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/login-form": "<div class=\"login-dropdown\" id=\"login-dropdown\">\r\n    <div class=\"login-header\">\r\n        <h2 class=\"login-title\">{{lang 'login.new_customer.heading2' }}</h2>\r\n    </div>\r\n    <div class=\"login-content\">\r\n        <form class=\"login-form form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n            {{inject 'useValidEmail' (lang 'forms.validate.common.email_address')}}\r\n            {{inject 'enterPass' (lang 'forms.validate.common.password')}}\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"login_email2\">{{lang 'login.field_email'}}</label>\r\n                <input class=\"form-input\" name=\"login_email\" id=\"login_email2\" type=\"email\">\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"form-label\" for=\"login_pass2\">{{lang 'login.field_password'}}</label>\r\n                <input class=\"form-input\" id=\"login_pass2\" type=\"password\" name=\"login_pass\">\r\n            </div>\r\n            <div class=\"form-actions\">\r\n                <input type=\"submit\" class=\"button button--border\" value=\"{{lang 'login.button' }}\">\r\n                <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n            </div>\r\n        </form>\r\n    </div>\r\n    <div class=\"new-customer\">\r\n        {{#if settings.account_creation_enabled}}\r\n            <a class=\"button\" href=\"{{urls.auth.create_account}}\">{{lang 'create_account.button' }}</a>\r\n        {{/if}}\r\n    </div>\r\n</div>\n",
  "components/common/navigation-list-alternate": "{{#if children}}\r\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root\" href=\"{{url}}\">\r\n        <span class=\"text\">\r\n            {{name}}\r\n            {{#if name '==' 'Buy ELLA'}}\r\n                <span class=\"navPages-label sale-label custom-label--sale\">Sale</span>\r\n            {{/if}}\r\n        </span>\r\n        <span class=\"navPages-action-moreIcon\">\r\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n            Back\r\n        </span>\r\n    </a>\r\n    {{> components/common/navigation-dropdown}}\r\n{{else}}\r\n    <a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\r\n{{/if}}\r\n",
  "components/common/social-links": "{{#or social_media theme_settings.social_icon_google_plus_link theme_settings.social_icon_snapchat_link}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#if social_media}}\n            {{#each social_media}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open {{display_name}} Social Media in a new tab\">\n                        Open in a new tab\n                        <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                </li>\n            {{/each}}\n        {{/if}}    \n        {{#if theme_settings.social_icon_google_plus_link '!=' ''}}\n            <li class=\"socialLinks-item google-plus\">\n                <a class=\"icon icon--google-plus\" href=\"{{theme_settings.social_icon_google_plus_link}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open Google Plus Social Media in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-google-plus\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\n        {{#if theme_settings.social_icon_snapchat_link '!=' ''}}\n            <li class=\"socialLinks-item snapchat\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.social_icon_snapchat_link}}\" target=\"_blank\" rel=\"noopener\" aria-label=\"Open Snapchat Social Media in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat-ghost\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n{{/or}}\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal level-2\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title navPages-action-end\" data-level=\"2\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"navPages-action-moreIcon\">\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                    Back\n                </span>\n                <span class=\"text\">{{name}}</span>\n            </a>\n        </li>\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-all navPages-action-end\" data-level=\"2\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n            </a>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child\" data-level=\"3\">\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu level-2\" href=\"{{url}}\">\n                        <span class=\"text\">{{name}}</span>\n                        <span class=\"navPages-action-moreIcon\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            Back\n                        </span>\n                    </a>\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal level-3\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n                         <ul class=\"navPage-subMenu-list\">\n                             <li class=\"navPage-subMenu-item-child navPage-subMenu-title navPages-action-end\" data-level=\"3\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"navPages-action-moreIcon\">\n                                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                        Back\n                                    </span>\n                                    <span class=\"text\">{{name}}</span>\n                                </a>\n                            </li>\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-all navPages-action-end\" data-level=\"3\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n                                </a>\n                            </li>\n                            {{#each children}}\n                                <li class=\"navPage-subMenu-item-child\" data-level=\"4\">\n                                    <a class=\"navPage-subMenu-action navPages-action level-3\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                            {{/each}}\n                         </ul>\n                    </div>\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\" data-level=\"3\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}