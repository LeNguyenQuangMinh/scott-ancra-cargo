{
  "components/brand/product-listing": "<div class=\"themevale_categoryToolbar\">\n    <div class=\"item item-iconSidebar\">\n        <div class=\"themevale_sidebar-mobile\">\n            <span class='form-label'>sidebar:</span>\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href='#sidebar'></use></svg>\n        </div>\n    </div>\n    <div class=\"item item-viewas\">\n        <div class=\"view-as-group\">\n            <label class=\"form-label\">{{lang 'common.sorter.view'}}</label>\n            <div class=\"view-as-btn\">\n                <a href=\"javascript:void(0);\" id=\"list-view\" data-layout=\"productGrid--maxCol1\" title=\"List View\"></a>\n                <a href=\"javascript:void(0);\" id=\"grid-view-2\" data-layout=\"productGrid--maxCol2\" title=\"Grid View 2\"></a>\n                <a href=\"javascript:void(0);\" id=\"grid-view-3\" data-layout=\"productGrid--maxCol3\" title=\"Grid View 3\"></a>\n                <a href=\"javascript:void(0);\" id=\"grid-view-4\" data-layout=\"productGrid--maxCol4\" title=\"Grid View 4\"></a>\n                <a href=\"javascript:void(0);\" id=\"grid-view-5\" data-layout=\"productGrid--maxCol5\" title=\"Grid View 5\"></a>\n            </div>\n        </div>\n    </div>\n    <div class=\"item item-filter\">\n        {{> components/products/filter sort=pagination.brand.sort products_per_page=theme_settings.brandpage_products_per_page}}\n    </div>\n</div>\n\n<form action=\"{{urls.compare}}\" method='POST' {{#if settings.data_tag_enabled}} data-list-name=\"Brand: {{brand.name}}\" {{/if}} data-product-compare>\n    {{> components/products/grid block_id=../block_id products=brand.products productsnew=../products.new show_compare=brand.show_compare theme_settings=theme_settings event=\"list\"}}\n</form>\n\n<div class=\"pagination-wrapper\"> \n    {{> components/common/paginator pagination.brand}}\n    {{#if brand.show_compare}}\n        <div class=\"compare-link\">\n            <a href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}}<span class=\"countPill countPill--positive countPill--alt\">0</span></a>\n        </div>\n    {{/if}}\n</div>\n",
  "components/products/filter": "<form class=\"actionBar\" method=\"get\" data-sort-by data-show-products>\n    <fieldset class=\"form-fieldset actionBar-section\">\n        <div class=\"form-field\">\n            <label class=\"form-label\" for=\"limit\">{{lang 'common.sorter.show'}}</label>\n            <select class=\"form-select\" name=\"limit\" id=\"limit\">\n                <option value=\"9\" {{#if '9' '==' products_per_page }}selected{{/if}}>9 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"12\" {{#if '12' '==' products_per_page }}selected{{/if}}>12 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"15\" {{#if '15' '==' products_per_page }}selected{{/if}}>15 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"20\" {{#if '20' '==' products_per_page }}selected{{/if}}>20 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"24\" {{#if '24' '==' products_per_page }}selected{{/if}}>24 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"30\" {{#if '30' '==' products_per_page }}selected{{/if}}>30 {{lang 'common.sorter.items_per_page'}}</option>\n            </select>\n        </div>\n    </fieldset>\n    <script>\n        function getUrlVars() {\n            var vars = {};\n            var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {\n                vars[key] = value;\n            });\n            return vars;\n        }\n        (function(){\n            try {\n                var c = getUrlVars()[\"limit\"];\n                if(c == undefined) c = {{theme_settings.categorypage_products_per_page}};\n                if(c != undefined){\n                    var limit = document.querySelectorAll('select#limit option');\n                    Array.prototype.forEach.call(limit, function(element) {\n                        if(element.value == c){\n                            element.selected = true;\n                        }\n                    });\n                }\n            } catch(e) {}\n        }());\n    </script>\n</form>\n<form class=\"actionBar\" method=\"get\" data-sort-by>\n    {{> components/category/sort-box sort=sort}}\n</form>\n\n",
  "components/products/grid": "<ul id=\"productLayout\" class=\"productGrid card-variant\">\n    {{#each products}}\n    <li class=\"product\">\n            {{> components/products/card-category productsnew=../productsnew settings=../settings show_compare=../show_compare show_wishlist=../settings.show_wishlist show_rating=../settings.show_product_rating theme_settings=../theme_settings customer=../customer event=../event position=(add @index 1)}}\n    </li>\n    {{/each}}\n</ul>\n",
  "components/common/paginator": "<div class=\"pagination\">\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\" href=\"{{previous}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\n                        </svg>\n                    </i>\n                    {{lang 'common.previous'}}\n                </a>\n            </li>\n        {{/if}}\n        {{#each links}}\n            {{#if this.number '==' ../current}}\n                <li class=\"pagination-item pagination-item--current\">\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\n                </li>\n            {{else}}\n                <li class=\"pagination-item\">\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\n                </li>\n            {{/if}}\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\" href=\"{{next}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\n                    {{lang 'common.next'}}\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg>\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\n                        </svg>\n                    </i>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</div>\n",
  "components/category/sort-box": "<fieldset class=\"form-fieldset actionBar-section\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n        <select class=\"form-select form-select--small\" name=\"sort\" id=\"sort\">\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\n            {{#if result_count}}\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n            {{/if}}\n        </select>\n    </div>\n</fieldset>\n\n",
  "components/products/card-category": "<article class=\"card {{#if alternate}}card--alternate{{/if}}\" data-product-id=\"{{id}}\" {{#if settings.data_tag_enabled}} data-event-type=\"{{event}}\" data-entity-id=\"{{id}}\" data-position=\"{{position}}\" data-name=\"{{name}}\" data-product-category=\"{{#each category}}{{#if @last}}{{this}}{{else}}{{this}}, {{/if}}{{/each}}\" data-product-brand=\"{{brand.name}}\" data-product-price=\"{{#if price.with_tax}}{{price.with_tax.value}}{{else}}{{price.without_tax.value}}{{/if}}\"  {{/if}}>\r\n    <figure class=\"card-figure\">\r\n        <a href=\"{{url}}\" class=\"product_img_link\" {{#if settings.data_tag_enabled}} data-event-type=\"product-click\" {{/if}}>\r\n            <div class=\"card-img-container {{#if images.length '>=' 2}}card-img-hover{{/if}}\">\r\n                <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                {{#if images.length '>=' 2}}\r\n                    {{#each images}}\r\n                        {{#if @index '==' 1}}\r\n                            <span class=\"product-additional\" data-product-id=\"\">\r\n                                <img class=\"replace-2x img-responsive lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage this 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                            </span>\r\n                        {{/if}}\r\n                    {{/each}}\r\n                {{/if}}\r\n            </div>\r\n        </a>\r\n    </figure>\r\n    <div class=\"card-body\">\r\n        <h4 class=\"card-title\" style=\"-webkit-box-orient: vertical\">\r\n            <a href=\"{{url}}\" {{#if settings.data_tag_enabled}} data-event-type=\"product-click\" {{/if}}>{{name}}</a>\r\n        </h4>\r\n        {{#each custom_fields}}\r\n            {{#if name '==' '__part'}}\r\n                <dd class=\"productView-info-value {{name}}\">{{{value}}}</dd>\r\n            {{/if}}\r\n        {{/each}}\r\n    </div>\r\n</article>\r\n"
}