{
  "components/themevale/footer/footer-layout-7": "<footer class=\"footer footer-layout-4 {{#if theme_settings.footer-dark-layout}}themevale-footer-dark{{/if}}\">\r\n    <div class=\"themevale_footer-top\">\r\n        <div class=\"container\">\r\n            <div class=\"item text-center\">\r\n                {{#if settings.show_newsletter_box}}\r\n                    <div class=\"subscription-form-5\">\r\n                        <h5 class=\"footer-info-heading\">Email Sign Up</h5>\r\n                        <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n                            <fieldset class=\"form-fieldset\">\r\n                                <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n                                <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n                                <input type=\"hidden\" name=\"check\" value=\"1\">\r\n                                <div class=\"form-field\">\r\n                                    <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n                                    <div class=\"form-prefixPostfix wrap\">\r\n                                        <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Email Address\">\r\n                                        <input class=\"button form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit_2'}}\">\r\n                                    </div>\r\n                                </div>\r\n                            </fieldset>\r\n                        </form>\r\n                    </div>\r\n                {{/if}}\r\n                <div class=\"item\">\r\n                    {{> components/common/social-links}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale_footer-middl